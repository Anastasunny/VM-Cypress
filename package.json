{
  "name": "vm-cypress",
  "description": "Cypress tests for Vacation manager application",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "cypress:open": "cypress open --config-file config/dev.json --browser firefox",
    "cypress:run": "cypress run --browser firefox --config-file config/dev.json --spec 'cypress/tests/**/*.ts'",
    "cy:verify": "cypress verify"
  },
  "devDependencies": {
    "cypress": "^9.1.0",
    "typescript": "~4.0.5"
  }
}
